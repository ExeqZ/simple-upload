{
  // This file provides parameters for the ARM template (azuredeploy.json). No Bicep files are used.
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountName": {
      "value": "<your-storage-account-name>"
    },
    "containerName": {
      "value": "<your-container-name>"
    },
    "location": {
      "value": "East US"
    },
    "webAppName": {
      "value": "<your-web-app-name>"
    },
    "appServicePlanName": {
      "value": "<your-app-service-plan-name>"
    }
  }
}